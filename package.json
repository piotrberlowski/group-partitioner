{
    "name": "group-planner",
    "version": "0.1.0",
    "description": "A TypeScript library for group planning functionality",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "type-check": "tsc --noEmit",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "clean": "rm -rf dist",
        "prepublishOnly": "npm run clean && npm run build"
    },
    "keywords": [
        "group",
        "planner",
        "typescript",
        "nextjs",
        "library"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "highs": "^1.7.0"
    },
    "devDependencies": {
        "@types/jest": "^29.5.8",
        "@types/node": "^20.10.0",
        "jest": "^29.7.0",
        "ts-jest": "^29.1.1",
        "tsup": "^8.0.1",
        "typescript": "^5.3.0"
    },
    "peerDependencies": {
        "react": ">=16.8.0"
    },
    "peerDependenciesMeta": {
        "react": {
            "optional": true
        }
    },
    "engines": {
        "node": ">=16.0.0"
    }
}